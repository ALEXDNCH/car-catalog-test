{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint . --ext .ts,.js,.vue",
    "lint:fix": "eslint . --fix --ext .ts,.js,.vue",
    "lint:strict": "eslint . --ext .ts,.js,.vue --max-warnings=0",
    "typecheck": "vue-tsc --noEmit",
    "prebuild": "npm run lint:strict && npm run typecheck",
    "dev:ssr": "nuxt dev",
    "dev:spa": "NUXT_SSR=false nuxt dev",
    "build:ssr": "npm run prebuild && nuxt build",
    "build:spa": "npm run prebuild && NUXT_SSR=false nuxt build",
    "preview": "nuxt preview",
    "generate": "nuxt generate",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "@nuxt/image": "^1.11.0",
    "@pinia/nuxt": "^0.11.2",
    "@tanstack/vue-query": "^5.90.2",
    "@vueuse/router": "^13.9.0",
    "nuxt": "^4.1.2",
    "pinia": "^3.0.3",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.9.0",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@tanstack/vue-query-devtools": "^5.90.2",
    "@types/node": "^24.5.2",
    "@vueuse/core": "^13.9.0",
    "@vueuse/nuxt": "^13.9.0",
    "eslint": "^9.36.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vue-tsc": "^3.0.8"
  }
}
